{
  "name": "donejs-example",
  "version": "0.2.0",
  "description": "An example project for DoneJS",
  "homepage": "https://github.com/tracer99/donejs-example",
  "repository": {
    "type": "git",
    "url": "git@github.com:tracer99/donejs-example.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "test": "DEBUG='testee:*' testee ./src/test.html -c ./config/testee.json",
    "start": "done-serve --port 8080",
    "develop": "./bin/dev_startup.js --development",
    "build": "node build",
    "lint": "npx eslint src/**/*.js --fix",
		"dev-bundle": "steal-tools bundle --dev"
  },
  "main": "donejs-example/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "donejs-app"
  ],
  "steal": {
    "main": "donejs-example/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "map": {
      "chai": "chai/chai",
      "sinon": "sinon/pkg/sinon"
    },
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "can-ajax": "^1.1.0",
    "can-component": "^3.3.5",
    "can-connect": "^1.5.9",
    "can-define": "^1.5.1",
    "can-list": "^3.2.1",
    "can-map": "^3.4.0",
    "can-map-define": "^3.1.1",
    "can-route": "^3.2.3",
    "can-route-pushstate": "^3.1.2",
    "can-set": "^1.3.2",
    "can-stache": "^3.10.1",
    "can-view-autorender": "^3.1.1",
    "can-zone": "^0.6.12",
    "done-autorender": "^1.3.2",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.4.0",
    "generator-donejs": "^1.0.7",
    "jquery": "^3.2.1",
    "steal": "^1.5.15",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.1.2"
  },
  "devDependencies": {
    "can-fixture": "^1.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "commander": "^2.11.0",
    "documentjs": "^0.4.2",
    "donejs-cli": "^1.0.2",
    "donejs-mocha": "^0.1.0",
    "eslint-config-tucows": "^1.0.4",
    "less": "^2.7.2",
    "sinon": "^1.17.6",
    "steal-mocha": "0.0.3",
    "steal-tools": "^1.9.1",
    "testee": "^0.3.0"
  },
  "license": "MIT"
}
