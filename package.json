{
  "name": "donejs-example",
  "version": "0.0.0",
  "description": "An example project for DoneJS",
  "homepage": "https://github.com/tracer99/donejs-example",
  "author": {
    "name": "Paul Tichonczuk",
    "email": "pault@tucows.com",
    "url": "http://tracer.ca"
  },
  "scripts": {
    "test": "testee src/test.html --browsers phantom --reporter Spec",
    "test-debug": "DEBUG='testee:*' testee src/test.html --browsers phantom --reporter Spec",
    "start": "done-serve --port 8080",
    "start_dev": "./bin/dev_startup.js --production",
    "develop": "./bin/dev_startup.js --development",
    "document": "documentjs",
    "build": "node build --production",
    "build_dev": "node build --development"
  },
  "main": "donejs-example/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [],
  "steal": {
    "main": "donejs-example/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "meta": {
      "core-js/client/core": {
        "format": "amd"
      }
    },
    "transpiler": "babel",
    "map": {
      "chai": "chai/chai",
      "sinon": "sinon/pkg/sinon"
    },
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "can": "^3.5.1",
    "can-connect": "^1.0.0",
    "can-zone": "^0.6.0",
    "done-autorender": "^1.0.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^0.2.0",
    "generator-donejs": "^1.0.0",
    "jquery": "~2.2.1",
    "jqueryui": "^1.11.1",
    "steal": "^1.5.0",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.0.5"
  },
  "devDependencies": {
    "can-fixture": "^1.0.13",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "connect": "^3.4.1",
    "documentjs": "^0.4.2",
    "donejs-cli": "^1.0.0",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-standard": "^1.3.2",
    "funcunit": "~3.2.0",
    "serve-static": "^1.11.1",
    "sinon": "^1.17.6",
    "steal-mocha": "0.0.3",
    "steal-tools": "^1.3.0",
    "testee": "^0.3.0"
  }
}
