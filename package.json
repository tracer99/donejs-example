{
  "name": "donejs-example",
  "version": "0.2.0",
  "description": "An example project for DoneJS",
  "homepage": "https://github.com/tracer99/donejs-example",
  "repository": {
    "type": "git",
    "url": "git@github.com:tracer99/donejs-example.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 8080",
    "develop": "done-serve --develop --port 8080",
    "build": "node build"
  },
  "main": "donejs-example/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "donejs-app"
  ],
  "steal": {
    "main": "donejs-example/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "map": {
      "chai": "chai/chai",
      "sinon": "sinon/pkg/sinon"
    },
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "can-component": "^3.0.7",
    "can-connect": "^1.3.8",
    "can-define": "^1.0.17",
    "can-list": "^3.0.5",
    "can-map": "^3.0.7",
    "can-map-define": "^3.0.9",
    "can-route": "^3.0.8",
    "can-route-pushstate": "^3.0.3",
    "can-set": "^1.1.0",
    "can-stache": "^3.0.20",
    "can-view-autorender": "^3.0.4",
    "can-zone": "^0.6.6",
    "done-autorender": "^1.0.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.0.0",
    "generator-donejs": "^1.0.0",
    "steal": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.0.5"
  },
  "devDependencies": {
    "donejs-cli": "^1.0.0",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0",
    "can-fixture": "^1.0.13",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "documentjs": "^0.4.2",
    "donejs-mocha": "^0.1.0",
    "sinon": "^1.17.6",
    "steal-mocha": "0.0.3"
  },
  "license": "MIT"
}
